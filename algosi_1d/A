static void partition()
{
    int n = int.Parse(Console.ReadLine());
    int[] a = new int[n];
    string[] b = Console.ReadLine().Split();
    for (int i = 0; i < a.Length; i++)
    {
        a[i] = int.Parse(b[i]);
    }
    int p = int.Parse(Console.ReadLine());
    int k; k = 0;
    foreach (int i in a)
    {
        if (i < p) { k++; }
    }
    Console.WriteLine(k);
    Console.WriteLine(n-k);
}
partition();
